{
  "name": "aem-sdk-setup",
  "version": "1.0.0",
  "description": "CLI utility to set up local AEM SDK environments using oclif.",
  "main": "index.js",
  "scripts": {
    "pretest": "npm run format",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier -w \"**/*.{js,json,md}\"",
    "prepare": "node bin/run --help",
    "prepack": "npm run format"
  },
  "keywords": [
    "aem",
    "cli",
    "sdk"
  ],
  "author": "AEM X",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/org/aem-sdk-setup.git"
  },
  "bugs": {
    "url": "https://github.com/org/aem-sdk-setup/issues"
  },
  "homepage": "https://github.com/org/aem-sdk-setup#readme",
  "files": [
    "bin",
    "src",
    "oclif.config.json",
    "README.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "type": "commonjs",
  "eslintIgnore": [
    "coverage"
  ],
  "dependencies": {
    "@oclif/core": "^4.3.3",
    "fs-extra": "^11.3.0",
    "glob": "^11.0.2",
    "unzipper": "^0.12.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^18.19.111",
    "eslint": "^9.28.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.1",
    "jest": "^30.0.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2"
  },
  "bin": {
    "aem-sdk-setup": "./bin/run.js"
  },
  "oclif": {
    "bin": "aem-sdk-setup",
    "commands": "./dist/commands",
    "dirname": "aem-sdk-setup",
    "topicSeparator": " "
  }
}
